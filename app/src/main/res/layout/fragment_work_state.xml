<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mm="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/c_ecedf1"
    android:orientation="vertical">


    <EditText
        android:id="@+id/text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="请输入" />

    <Button
        android:id="@+id/btn_ok"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="是/否要登录" />

    <Button
        android:id="@+id/btn_etcp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="scheme为etcp://"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_http"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="scheme为http://"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_etcp_s_e"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="scheme为etcp:// url=etcp"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_etcp_s_h"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="scheme为etcp:// url=http"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_etcp_s_d_e"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="使用defaultData, scheme为etcp"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_etcp_s_d_h"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="使用defaultData,scheme为http"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_etcp_s_d_error"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="错误的defaultData"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_etcp_s_d_error_o"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="错误的defaultData"
        android:textAllCaps="false" />

    <TextView
        android:id="@+id/tv_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <com.ddu.icore.ui.view.CustomerTimeLineMarker
        android:id="@+id/item_time_line_mark"
        android:layout_width="50dp"
        android:layout_height="@dimen/dp_300"
        android:layout_gravity="center"
        mm:beginLine="@color/c_cccccc"
        mm:endLine="@color/c_cccccc"
        mm:lineSize="1dp"
        mm:marker="@drawable/ic_timeline_default_marker"
        mm:markerSize="7dp" />
</LinearLayout>
